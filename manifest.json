{
        "manifest_version": 2,
        "name": "ichneumon",
        "version": "1.4",
        "description": "JavaScript Inject",
        "author": "Lee",
        "icons":
        {
                "16": "img/icon.png",
                "48": "img/icon.png",
                "128": "img/icon.png"
        },
        "browser_action":
        {
                "default_icon": "img/icon.png",
                "default_title": "JavaScript Inject",
                "default_popup": "html/main.html"
        },
        "content_scripts":
        [
                {
                        "matches":["*://*/*"],
                        "js":["js/onclickurl1.js"],
                        "run_at":"document_end"
                }
        ],
        "background":
	{
		"page": "html/background.html"
	},

        "permissions":
	[
		"contextMenus", // 右键菜单
		"tabs", // 标签
		"notifications", // 通知
		"webRequest", // web请求
		"webRequestBlocking",
		"storage", // 插件本地存储
                "*://*/*",
		"http://*/*", // 可以通过executeScript或者insertCSS访问的网站
		"https://*/*", // 可以通过executeScript或者insertCSS访问的网站
                "activeTab",
                "browsingData",
                "chrome://favicon/",
                "clipboardWrite",
                "cookies",
                "idle",
                "storage",
                "unlimitedStorage"
	]
}